{
    "name": "crayoon/gotask",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "A replacement for Swoole TaskWorker in Go. see hyperf/gotask",
    "autoload": {
        "psr-4": {
            "Hyperf\\GoTask\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.4",
        "hyperf/pool": "^2.0.0",
        "hyperf/process": "^2.0.0",
        "spiral/goridge": "^2.4.1",
        "symfony/event-dispatcher": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/command": "^2.0.0",
        "hyperf/config": "^2.0.0",
        "hyperf/di": "^2.0.0",
        "hyperf/framework": "^2.0.0",
        "hyperf/testing": "^2.0.0",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12",
        "swoole/ide-helper": "^4.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
    },
    "extra": {
    }
}
